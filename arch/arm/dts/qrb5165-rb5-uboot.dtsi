/*
 * U-Boot addition to handle QRB5165-RB5 pre-relocation devices
 *
 * (C) Copyright 2023 Bhupesh Sharma <bhupesh.sharma@linaro.org>
 */

/ {
	soc {
		u-boot,dm-pre-reloc;

		pinctrl_west@f100000 {
			u-boot,dm-pre-reloc;
		};

		clock-controller@100000 {
			u-boot,dm-pre-reloc;
		};

		serial@988000 {
			u-boot,dm-pre-reloc;
		};
	};
};

&pm8150_pon {
	key_vol_down {
		gpios = <&pm8150_pon 1 0>;
		label = "key_vol_down";
	};
	key_power {
		gpios = <&pm8150_pon 0 0>;
		label = "key_power";
	};
};
